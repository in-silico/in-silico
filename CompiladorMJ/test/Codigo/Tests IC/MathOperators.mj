/*
 * An array which can Bubble sort it's elements.
 * Code originated from java:
 * http://www.samspublishing.com/articles/article.asp?p=31526&seqNum=2&rl=1
 */
class ArrayBub {

	static void main(string[] args) 
	{
		int[] arr = new int[1];
		int var = 7;
		int var2 = 5;
		int a1 = 1;
		int a2 = 2;
		int a3 = 3;
		int a4 = 4;
		int a5 = 5;

		TestClass field = new TestClass();
		field.b = 6;

		arr[0] = 7;
		
		if (9 + 4 != 13)
			Library.println("error1 in +");

		if ((4 - 9 != -5)
				|| (9 - 4 != 5))
			Library.println("error1 in -");

		if (9 * 4 != 36)
			Library.println("error1 in *");

		if (12 / 4 != 3)
			Library.println("error1 in /");

		if (21 % 4 != 1)
			Library.println("error1 in %");

		if (-1 != 0 - 1)
			Library.println("error1 in the unary operator -");
			

			
		if (var + 4 != 11)
			Library.println("error2.1 in +");

		if (4 + var != 11)
			Library.println("error2.2 in +");
			
		if ((4 - var != -3)
				|| (var - 4 != 3))
			Library.println("error2 in -");

		if (var * 4 != 28)
			Library.println("error2 in *");
			
		if (4 * var != 28)
			Library.println("error2 in *");

		if (22 / var != 3)
			Library.println("error2 in /");

		if (var % 4 != 3)
			Library.println("error2 in %");

		if (-var != 0 - 7)
			Library.println("error2 in the unary operator -");
			
		if (var - var2 != 2)
			Library.println("error2.2 in -");

			
		if (arr[0] + 4 != 11)
			Library.println("error3 in +");

		if ((4 - arr[0] != -3)
				|| (arr[0] - 4 != 3))
			Library.println("error3 in -");

		if (arr[0] * 4 != 28)
			Library.println("error3 in *");

		if (22 / arr[0] != 3)
			Library.println("error3 in /");

		if (arr[0] % 4 != 3)
			Library.println("error3 in %");

		if (-arr[0] != 0 - 7)
			Library.println("error3 in the unary operator -");
								
		if (field.b + 4 != 10)
			Library.println("error4 in +");

		if ((4 - field.b != -2)
				|| (field.b - 4 != 2))
			Library.println("error4 in -");

		if (field.b * 4 != 24)
			Library.println("error4 in *");

		if (22 / field.b != 3)
			Library.println("error4 in /");

		if (field.b % 4 != 2)
			Library.println("error4 in %");

		if (-field.b != 0 - 6)
			Library.println("error4 in the unary operator -");

		Library.println("Testing long Expretions");

		arr[0] = 6;
		
		if (a1 + a2 * a3 - a4 != 3)
			Library.println("error1 in complex operators");
			
		if(arr[0] + 5 * 5 * a2 - arr[0] / a2 != 53)
			Library.println("error2 in complex operators");
			
		Library.print("Hello World");
	}
}

class TestClass
{
	int b;
}

class Library {
 static void println(string s){} /* prints string s followed by a newline */
 static void print(string s){}   /* prints string s */
 static void printi(int i){}     /* prints integer i */
 static void printb(boolean b){} /* prints boolean b */

 static int readi(){}     /* reads an integer from the input */
 static string readln(){} /* reads one line from the input */
 static boolean eof(){}   /* checks end-of-file on standard input */

 static int stoi(string s, int n){} /* returns the integer that s represents
                                      or n of s is not an integer */
 static string itos(int i){}   /* returns a string representation of i */
 static int[] stoa(string s){} /* an array with the ascii codes of chars in s */
 static string atos(int[] a){} /* builds a string from the ascii codes in a */

 static int random(int i){} /* returns a random number between 0 and n-1 */
 static int time(){}        /* number of milliseconds since program start */
 static int exit(int i){}   /* terminates the program with exit code n */
}
